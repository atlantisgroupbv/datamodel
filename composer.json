{
  "name": "atlantisgroupbv/ormmodel",
  "homepage": "https://github.com/skooijman/ormmodel",
  "description": "Custom orm with multiple drivers",
  "keywords": ["DB2Model", "DB2ORM"],
  "license": "MIT",
  "authors": [
    {
      "name": "Bas Kooijman",
      "email": "bas.kooijman@gmail.com",
      "role": "Creator / Main Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/validator": "~6.1",
    "doctrine/annotations": "~1.0",
    "doctrine/cache": "~1.0",
    "symfony/intl": "~6.1",
    "symfony/config": "~6.1",
    "symfony/yaml": "~6.1"
  },
  "require-dev": {
    "psr/log": "^1.0",
    "psr/container": "^1.0",
    "phpunit/phpunit": "*",
    "squizlabs/php_codesniffer": "*",
    "pdepend/pdepend": "*",
    "phpmd/phpmd": "*",
    "phploc/phploc": "*",
    "sebastian/phpcpd": "*",
    "phing/phing": "*"
  },
  "autoload": {
    "psr-4": {
      "atlantisgroupbv\\ormmodel\\": "src/"
    },
    "psr-0": {
      "Symfony\\Compoment\\Validator\\Constraints\\": "src/Validator/Constraints"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Test\\AtlantisGroupBV\\OrmModel\\": "test/src/"
    },
    "files": ["test/fixtures/classes.php"]
  },
  "bin": ["bin/ormmodel"],
  "suggest": {
    "rdlowrey/auryn": "Can be used to autowire the services, version ~1.4"
  }
}